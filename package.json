{
  "name": "@pureprofile/pg-schema-dump",
  "version": "1.0.3",
  "description": "",
  "author": "",
  "license": "MIT",
  "homepage": "https://github.com/pureprofile/pg-schema-dump#readme",
  "bugs": {
    "url": "https://github.com/pureprofile/pg-schema-dump/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pureprofile/pg-schema-dump.git"
  },
  "main": "./dist/index.js",
  "bin": {
    "pg-schema-dump": "./dist/bin.js"
  },
  "files": [
    "/dist",
    "!/dist/__tests__"
  ],
  "scripts": {
    "start": "npm run build && node ./dist/bin.js",
    "prepublishOnly": "npm test",
    "test": "npm run build && npm run eslint && npm run jest",
    "build": "rimraf ./dist && tsc",
    "eslint": "eslint --ext=ts,tsx ./src",
    "jest": "jest && npm run jest:badges",
    "jest:one": "jest",
    "jest:badges": "node_modules/.bin/jest-badges-readme"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --ext=ts,tsx --fix",
      "prettier --write"
    ],
    "*.{js,jsx,json}": [
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": "pureprofile",
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "rules": {
      "no-unused-expressions": "off"
    }
  },
  "prettier": "eslint-config-pureprofile/prettier-config",
  "dependencies": {
    "auto-bind": "^4.0.0",
    "fs-extra": "^9.0.1",
    "lodash": "^4.17.19",
    "pg": "^8.3.0",
    "yargs": "^15.4.0"
  },
  "devDependencies": {
    "@olavoparno/jest-badges-readme": "^1.5.1",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^26.0.4",
    "@types/lodash": "^4.14.157",
    "@types/node": "^14.0.20",
    "@types/pg": "^7.14.4",
    "@types/yargs": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-config-pureprofile": "^3.3.5",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.1",
    "typescript": "^3.9.6"
  }
}
